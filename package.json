{
  "name": "ng-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm:start:server:mock\" \"npm:start:proxy\"",
    "build": "ng build",
    "test": "jest",
