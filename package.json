    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "concurrently": "^4.0.1",
    "cypress": "^3.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "23.6.0",
    "jest-marbles": "^2.0.0",
    "jest-preset-angular": "6.0.1",
